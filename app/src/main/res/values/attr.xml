<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义Widget－仿“探探”效果-->
    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference" />
        <attr name="rotation_degrees" format="float" />
        <attr name="y_offset_step" format="dimension" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>
    <!-- 仿系统ScrollingViewBehavior-->
    <declare-styleable name="Follow">
        <attr name="target" format="reference"/>
    </declare-styleable>
    <!-- 自定义Widget—FlowLayout属性-->
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <!-- View构造函数的自定属性继承关系测试-->
    <declare-styleable name="Customize">
        <attr name="attr_one" format="string" />
        <attr name="attr_two" format="string" />
        <attr name="attr_three" format="string" />
        <attr name="attr_four" format="string" />
    </declare-styleable>
    <attr name="CustomizeStyle" format="reference" />

    <!-- 自定义View－CustomVerificationCodeView-->
    <attr name="titleStyle" format="reference"/>
    <attr name="titleText" format="string"/>
    <attr name="titleColor" format="color"/>
    <attr name="titleSize" format="dimension"/>
    <declare-styleable name="CustomVerificationCodeView">
        <attr name="titleText"/>
        <attr name="titleColor"/>
        <attr name="titleSize"/>
    </declare-styleable>
    <!-- 自定义View－CustomImageView-->
    <attr name="image" format="reference"/>
    <attr name="imageScaleType">
        <enum name="fillXY" value="0"/>
        <enum name="center" value="1"/>
    </attr>
    <declare-styleable name="CustomImageView">
        <attr name="titleText"/>
        <attr name="titleColor"/>
        <attr name="titleSize"/>
        <attr name="image"/>
        <attr name="imageScaleType"/>
    </declare-styleable>
    <!-- 自定义View－CustomProgressBar-->
    <attr name="firstColor" format="color" /><!--背景色-->
    <attr name="secondColor" format="color" /><!--前景色-->
    <attr name="circleWidth" format="dimension" /><!--圆环宽度-->
    <attr name="speed" format="integer" /><!--转动速度-->
    <declare-styleable name="CustomProgressBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="speed" />
    </declare-styleable>
    <!-- 自定义View－CustomVolumeControlBar-->
    <attr name="dotCount" format="integer" /><!--描点数量-->
    <attr name="splitSize" format="integer" /><!--每个点指间的间隔-->
    <attr name="bg" format="reference"></attr><!--中间的图片-->
    <declare-styleable name="CustomVolumeControlBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="dotCount" />
        <attr name="splitSize" />
        <attr name="bg" />
    </declare-styleable>
    <!-- 自定义Widget－手势密码－GestureLockView-->
    <attr name="color_no_finger_inner_circle" format="color"/><!--无手指触摸状态下的内圆颜色-->
    <attr name="color_no_finger_outer_circle" format="color"/><!--无手指触摸状态下的外圆颜色-->
    <attr name="color_finger_on" format="color"/><!--手指触摸的状态下内圆和外圆的颜色 -->
    <attr name="color_finger_up_error" format="color"/><!--手指抬起的状态下内圆和外圆的颜色(验证错误) -->
    <attr name="color_finger_up_right" format="color"/><!--手指抬起的状态下内圆和外圆的颜色(验证正确) -->
    <attr name="count" format="integer"/><!--矩阵个数-->
    <attr name="tryTimes" format="integer"/><!--尝试次数-->
    <attr name="minPoint" format="integer"/><!--最小手势点个数-->
    <declare-styleable name="GestureLockViewGroup">
        <attr name="color_no_finger_inner_circle"/>
        <attr name="color_no_finger_outer_circle"/>
        <attr name="color_finger_on"/>
        <attr name="color_finger_up_error"/>
        <attr name="color_finger_up_right"/>
        <attr name="count"/>
        <attr name="tryTimes"/>
        <attr name="minPoint"/>
    </declare-styleable>
    <!--自定义View－自定义属性－CustomStateView-->
    <attr name="state_1" format="boolean"/>
    <attr name="state_2" format="boolean"/>
    <attr name="state_3" format="boolean"/>
    <declare-styleable name="CustomStateView">
        <attr name="state_1"/>
        <attr name="state_2"/>
        <attr name="state_3"/>
    </declare-styleable>
    <!--自定义Widget－ArcMenu－圆弧动画菜单-->
    <attr name="position" format="enum"><!--位置-->
        <enum name="left_top" value="0"/>
        <enum name="right_top" value="1"/>
        <enum name="left_bottom" value="2"/>
        <enum name="right_bottom" value="3"/>
    </attr>
    <attr name="radius" format="dimension"/><!--半径-->
    <declare-styleable name="ArcMenu">
        <attr name="position"/>
        <attr name="radius"/>
    </declare-styleable>
    <!--自定义Widget－SlidingMenu－侧滑-->
    <attr name="rightPadding" format="dimension"/><!--距屏幕右边距离-->
    <declare-styleable name="SlidingMenu">
        <attr name="rightPadding"/>
    </declare-styleable>
    <!--自定义Widget－ClipImageLayout－裁剪Image-->
    <attr name="horizontal_padding" format="dimension"/>
    <attr name="src" format="reference"/>
    <declare-styleable name="ClipImageLayout">
        <attr name="horizontal_padding"/>
        <attr name="src"/>
    </declare-styleable>
    <!--自定义Widget－CircleImageView－裁剪Image-->
    <attr name="imageRadius" format="dimension"/><!--半径-->
    <attr name="type" format="enum">
        <enum name="circle" value="0"/><!--圆形-->
        <enum name="round" value="1"/><!--圆角-->
    </attr>
    <attr name="imageSrc" format="reference"/>
    <declare-styleable name="CircleImageView">
        <attr name="imageRadius"/>
        <attr name="type"/>
        <attr name="imageSrc"/>
    </declare-styleable>
    <!--自定义View－ChangeColorIconWithTextView－仿微信底部tab-->
    <attr name="bottom_icon" format="reference"/>
    <attr name="bottom_color" format="color"/>
    <attr name="text" format="string"/>
    <attr name="text_size" format="dimension"/>
    <declare-styleable name="ChangeColorIconWithTextView">
        <attr name="bottom_icon"/>
        <attr name="bottom_color"/>
        <attr name="text"/>
        <attr name="text_size"/>
    </declare-styleable>
    <!--自定义View－BottomItemChangeColor－仿微信底部tab -->
    <attr name="bottom_item_front_icons" format="reference"></attr>
    <attr name="bottom_item_front_color" format="color"></attr>
    <attr name="bottom_item_back_icons" format="reference"></attr>
    <attr name="bottom_item_back_color" format="color"></attr>
    <attr name="bottom_item_text" format="string"></attr>
    <attr name="bottom_item_text_size" format="dimension"></attr>
    <declare-styleable name="BottomItemChangeColor">
        <attr name="bottom_item_front_icons"></attr>
        <attr name="bottom_item_front_color"></attr>
        <attr name="bottom_item_back_icons"></attr>
        <attr name="bottom_item_back_color"></attr>
        <attr name="bottom_item_text"></attr>
        <attr name="bottom_item_text_size"></attr>
    </declare-styleable>
    <!--自定义View－progressBar－自定义进度条样式-->
    <declare-styleable name="HorizontalProgressBarWithNumber">
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />
        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_offset" format="dimension" />
        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundProgressBarWidthNumber">
        <attr name="progress_radius" format="dimension" />
    </declare-styleable>
    <!--自定义属性－MyStateRelativeLayout-->
    <declare-styleable name="MyStateRelativeLayout">
        <attr name="state_message_readed" format="boolean"/>
    </declare-styleable>
    <!--自定义View－ColorTrackView－仿今日头条的tab字体颜色切换-->
    <attr name="color_track_text" format="string"/>
    <attr name="color_track_text_size" format="dimension"/>
    <attr name="color_track_origin_color" format="color|reference"/>
    <attr name="color_track_change_color" format="color|reference"/>
    <attr name="color_track_progress" format="float"/>
    <attr name="color_track_direction" format="enum">
        <enum name="left" value="0"/>
        <enum name="right" value="1"/>
    </attr>
    <declare-styleable name="ColorTrackView">
        <attr name="color_track_text"/>
        <attr name="color_track_text_size"/>
        <attr name="color_track_origin_color"/>
        <attr name="color_track_change_color"/>
        <attr name="color_track_progress"/>
        <attr name="color_track_direction"/>
    </declare-styleable>
    <!--自定义View—WaveView-仿探探的水波为效果-->
    <attr name="ripple_color" format="color|reference"/><!--波纹颜色-->
    <attr name="ripple_auto" format="boolean"/><!--是否自动生成波纹-->
    <attr name="ripple_fill" format="boolean"/><!--波纹是否填充-->
    <declare-styleable name="RippleView">
        <attr name="ripple_color"/>
        <attr name="ripple_auto"/>
        <attr name="ripple_fill"/>
    </declare-styleable>
    <!--自定义View－DynamicWave－波浪效果-->
    <declare-styleable name="DynamicWave">
        <attr name="wave_color" format="color|reference"/><!--波浪颜色-->
    </declare-styleable>
    <!--自定义View－CustomCoordinateAxisView－雷达坐标图-->
    <attr name="axis_color" format="color|reference"/><!--坐标轴线颜色-->
    <attr name="axis_fill_color" format="color|reference"/><!--围成面积的填充色-->
    <attr name="axis_text_bg_color" format="color|reference"/><!--文字颜色-->
    <attr name="axis_text_color" format="color|reference"/><!--文字背景颜色-->
    <attr name="axis_circle_color" format="color|reference"/><!--选中圆圈边色-->
    <attr name="axis_text_size" format="dimension"/><!--文字大小-->
    <declare-styleable name="AxisView">
        <attr name="axis_color"/><!--坐标轴线颜色-->
        <attr name="axis_fill_color"/><!--围成面积的填充色-->
        <attr name="axis_text_bg_color"/><!--文字颜色-->
        <attr name="axis_text_color"/><!--文字背景颜色-->
        <attr name="axis_circle_color"/><!--选中圆圈边色-->
        <attr name="axis_text_size"/><!--文字大小-->
    </declare-styleable>
    <!--自定义View－ColourImageView－填充颜色-->
    <attr name="colour_border_color" format="color|reference"/><!--边界颜色-->
    <declare-styleable name="ColourImageView">
        <attr name="colour_border_color"/>
    </declare-styleable>
    <!--双向选择的SeekBar－MaterialRangeSlider-->
    <declare-styleable name="MaterialRangeSlider">
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="unpressedTargetRadius" format="dimension" />
        <attr name="pressedTargetRadius" format="dimension" />
        <attr name="targetColor" format="color" />
        <attr name="insideRangeLineColor" format="color" />
        <attr name="outsideRangeLineColor" format="color" />
        <attr name="insideRangeLineStrokeWidth" format="dimension" />
        <attr name="outsideRangeLineStrokeWidth" format="dimension" />
    </declare-styleable>
    <!--双向选择的SeekBar－RangeSeekBar-->
    <declare-styleable name="RangeSeekBar">
        <attr name="max_seek_bar" format="float"/>
        <attr name="min_seek_bar" format="float"/>
        <attr name="reserve" format="float"/><!--保留距离支持：如果cells值为1，那么reserve最小保留1；此处cells分隔的最小单元为5，reserve将四舍五入为5-->
        <attr name="cells" format="integer"/><!--刻度支持：将最大值和最小值四等分，比如1-20分成5份即0，5，10，15，20-->
        <attr name="lineColorSelected" format="color"/><!--选中范围颜色-->
        <attr name="lineColorEdge" format="color"/><!--边缘配色-->
        <attr name="seekBarResId" format="reference"/><!--按钮样式-->
    </declare-styleable>
    <!--模糊渐变效果－BlurredView-->
    <declare-styleable name="BlurredView">
        <attr name="android:src"/><!--TODO yuyang 在自定义属性中使用Android自带的属性名字-->
        <attr name="android:scaleType"/>
    </declare-styleable>
    <!--ListView自定义侧滑控制-->
    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge"><!--从那边侧滑-->
            <flag name="left" value="1"/>
            <flag name="right" value="2"/>
            <flag name="top" value="4"/>
            <flag name="bottom" value="8"/>
        </attr>
        <attr name="leftEdgeSwipeOffset" format="dimension"/><!--这四个值一般不要设置-->
        <attr name="rightEdgeSwipeOffset" format="dimension"/>
        <attr name="topEdgeSwipeOffset" format="dimension"/>
        <attr name="bottomEdgeSwipeOffset" format="dimension"/>
        <attr name="show_mode" format="enum"><!--布局结构:lay_down－上下两层布局，pull_out—平面布局-->
            <enum name="lay_down" value="0"/><!--这个属性绘制的不好，不要用-->
            <enum name="pull_out" value="1"/>
        </attr>
        <attr name="clickToClose" format="boolean"/>
    </declare-styleable>
    <!--Banner指示器-->
    <declare-styleable name="CircleIndicator">
        <attr name="ci_width" format="dimension"/><!--drawable宽高-->
        <attr name="ci_height" format="dimension"/>
        <attr name="ci_margin" format="dimension"/><!--drawable的间距-->
        <attr name="ci_animator" format="reference"/><!--选中是的动画-->
        <attr name="ci_animator_reverse" format="reference"/><!--恢复为选中时的动画-->
        <attr name="ci_drawable" format="reference"/><!--选中时的drawable-->
        <attr name="ci_drawable_unselected" format="reference"/><!--为选中时现实的drawable-->
        <attr name="ci_orientation" format="enum"><!--设置指示器如何排列-->
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="ci_gravity"><!--设置指示器的布局分布（相当于在LinearLayout中的布局分布）-->
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="center_vertical" value="0x10"/>
            <flag name="fill_vertical" value="0x70"/>
            <flag name="center_horizontal" value="0x01"/>
            <flag name="fill_horizontal" value="0x07"/>
            <flag name="center" value="0x11"/>
            <flag name="fill" value="0x77"/>
            <flag name="clip_vertical" value="0x80"/>
            <flag name="clip_horizontal" value="0x08"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
    </declare-styleable>
    <!--ParallaxContainer ViewPager页面元素动画-->
    <attr name="a_in" format="float" />
    <attr name="a_out" format="float" />
    <attr name="x_in" format="float" />
    <attr name="x_out" format="float" />
    <attr name="y_in" format="float" />
    <attr name="y_out" format="float" />
    <!--LetterBarView－字母导航条-->
    <declare-styleable name="LetterBar">
        <attr name="lbLetterBarBackground" format="reference|color" /><!-- 侧边栏Bar的背景色 -->
        <attr name="lbLetterBarTextColor" format="reference|color" /><!-- 导航条文字颜色 -->
        <attr name="lbOverlayBackground" format="reference|color" /><!-- 选中的字母显示背景 -->
        <attr name="lbOverlayTextColor" format="reference|color" /><!-- 选中的字母显示文字颜色 -->
        <attr name="lbOverlayTextSize" format="reference|dimension" /><!-- 选中的文字大小 -->
    </declare-styleable>
    <!--WaveSideBar－波浪字母导航条-->
    <declare-styleable name="WaveSideBar">
        <attr name="sidebar_lazy_respond" format="boolean"/><!--是否延时发送回调-->
        <attr name="sidebar_text_color" format="color"/><!--字母颜色-->
        <attr name="sidebar_max_offset" format="dimension" /><!--波峰最大偏移量-->
        <attr name="sidebar_position" format="enum"><!--字母条靠左还是靠右显示-->
            <enum name="right" value="0"/>
            <enum name="left" value="1"/>
        </attr>
    </declare-styleable>
    <!--RadarChartView－雷达图属性-->
    <declare-styleable name="RadarChart">
        <attr name="polygons_text_size" format="dimension|reference"/><!--文字大小-->
        <attr name="polygons_text_color" format="color|reference"/><!--文字颜色-->
        <attr name="polygons_bg_color" format="color|reference"/><!--最外层背景色-->
        <attr name="polygons_line_color" format="color|reference"/><!--进度线颜色-->
        <attr name="polygons_datas" format="reference"/><!--数据名称集合-->
        <attr name="polygons_view_line_width" format="dimension|reference"/><!--图形线条宽度-->
        <attr name="polygons_value_line_width" format="dimension|reference"/><!--进度线条宽度-->
    </declare-styleable>
</resources>